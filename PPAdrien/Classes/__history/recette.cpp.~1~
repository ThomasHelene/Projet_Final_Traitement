#pragma package(smart_init)
					Recette::Recette(){
							this->temp1 = 0;
							this->temp2 = 0;
							this->temp3 = 0;

					}
						Recette::~Recette(){
						}


					void Recette::creerRecette(int temp1Base,int temp2Base,int temp3Base,int id){
							this->temp1 = temp1Base;
							this->temp2 = temp2Base;
							this->temp3 = temp3Base;

					}


